def is_abundant(number):
    # Find all proper divisors of the number
    proper_divisors = [i for i in range(1, number) if number % i == 0]
    
    # Calculate the sum of proper divisors
    sum_of_divisors = sum(proper_divisors)
    
    # Check if the sum of proper divisors is greater than the number itself
    if sum_of_divisors > number:
        return "Yes"
    else:
        return "No"

# Read input from stdin
number = int(input().strip())

# Print the result
print(is_abundant(number))
    
